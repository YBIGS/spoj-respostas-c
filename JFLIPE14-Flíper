#include <iostream>
using namespace std;

int main(){

    int A, B, C, P, R;

    cin >> P;
    cin >> R;

    if(P==0 && R==0){
      cout<< "C" << "\n";
    }
    if(P==0 && R==1){
      cout<< "C" << "\n";
    }
    if(P==1 && R==1){
      cout<< "A" << "\n";
    }

    if(P==1 && R==0){
      cout<< "B" << "\n";
    }
    
  }
