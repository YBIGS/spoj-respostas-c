#include <iostream>
using namespace std;

int main() {
    int a;

    setlocale(LC_ALL, "Portuguese");

    cout << "\n" << "Informe a nota: ";
    cin >> a;

    if (a >= 0 && a <= 100){
      if (a == 0)
        cout << "\n" << "E" << "\n" << endl;

      if (a > 0 && a < 36)
        cout << "\n" << "D" << "\n" << endl;

      if (a > 35 && a < 61)
        cout << "\n" << "C" << "\n" << endl;

      if (a > 60 && a < 86)
        cout << "\n" << "B" << "\n" << endl;

      if (a > 85 && a < 101)
        cout << "\n" << "A" << "\n" << endl;
    }
    else
      cout << "\n" << "Informe uma nota dentro dos padrÃµes." << "\n" << endl;

    }
